        <ФИООтв Фамилия="string" Имя="string" Отчество="string"/>
    </ИдОтпр>
    <КодНОПост>stri</КодНОПост>
    <!--1 or more repetitions:-->
    <Документ ИдДок="string">
        <СвОргРег НаимОрг="string" ИННЮЛ="stringstri" КПП="stringstr" ОГРН="stringstrings"/>
        <СвЗамВыд>
            <ДатаПостНО>stringstri</ДатаПостНО>
            <Выд_Зам_Утрат>
                <!--You have a CHOICE of the next 2 items at this level-->
                <Замена ПризнДок="s" ПричЗамДок="st" ДатаДокФакт="stringstri">
                    <СведФЛ ДатаРожд="stringstri" МестоРожд="string" Пол="1" ПрАдрМЖ="s" КодКладр="string"
