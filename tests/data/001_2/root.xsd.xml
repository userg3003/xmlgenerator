                        <АдресМЖ КодРегион="st" Город="string" Район="string" НаселПункт="string" Улица="string"
                                 Дом="string" Корпус="string" Кварт="string"/>
                        <АдресМЖКЛАДР КодРегион="st" Город="string" Район="string" НаселПункт="string" Улица="string"
                                      Дом="string" Корпус="string" Кварт="string"/>
                    </СведФЛ>
                    <ВыдДок КодВидДок="st" СерНомДок="string" ДатаУдЛичн="stringstri" КемВыдДок="string"
                            КодПодрВыдДок="strings"/>
                    <ЗамДок КодВидДок="st" СерНомДок="string" ДатаУдЛичн="stringstri" КемВыдДок="string"
                            КодПодрВыдДок="strings"/>
                    <!--Optional:-->
                    <ЗамРекв ДатаРожд="stringstri" МестоРожд="string" Пол="1">
                        <ФИОФЛ Фамилия="string" Имя="string" Отчество="string"/>
                    </ЗамРекв>
                </Замена>
                <ПервВыдача ПризнДок="s" ПричВыдДок="st" ДатаДокФакт="stringstri">
                    <СведФЛ ДатаРожд="stringstri" МестоРожд="string" Пол="1" ПрАдрМЖ="s" КодКладр="string"
                            АдрМЖРФТекст="string">
                        <ФИОФЛ Фамилия="string" Имя="string" Отчество="string"/>
                        <АдресМЖ КодРегион="st" Город="string" Район="string" НаселПункт="string" Улица="string"
                                 Дом="string" Корпус="string" Кварт="string"/>
                        <АдресМЖКЛАДР КодРегион="st" Город="string" Район="string" НаселПункт="string" Улица="string"
                                      Дом="string" Корпус="string" Кварт="string"/>
                    </СведФЛ>
                    <ВыдДок КодВидДок="st" СерНомДок="string" ДатаУдЛичн="stringstri" КемВыдДок="string"
                            КодПодрВыдДок="strings"/>
                    <УдЛичн КодВидДок="st" СерНомДок="string" ДатаУдЛичн="stringstri" КемВыдДок="string"
                            КодПодрВыдДок="strings"/>
                    <!--Optional:-->
                    <ЗамРекв ДатаРожд="stringstri" МестоРожд="string" Пол="1">
                        <ФИОФЛ Фамилия="string" Имя="string" Отчество="string"/>
                    </ЗамРекв>
                </ПервВыдача>
            </Выд_Зам_Утрат>
        </СвЗамВыд>
    </Документ>
